import java.awt.Frame;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;
import javax.imageio.ImageIO;

public static void main(String[] args) {

public class DashboardFrame extends Frame {
 
 /*Imports dashboard frame image*/
    Image img;
 
 public void DashboardFrame()
 {
/*Loads the background image and stores in img object*/
        img = Toolkit.getDefaultToolkit().createImage("background.jpg");
 }

 public void paint(Graphics g)
    {
        // Draws the img to the DashboardFrame.
        g.drawImage(img, 0, 0, null);
    }
 
 
 /*Imports a car*/
 
 Car myCar = new Car("car.png");
 myCar.updatePosition(450, 450);
 myCar.setMass(1.0f);
 myCar.setMaxSpeed(100.0f);
 myCar.setMaxSteering(70.0f);
 myCar.addBehaviour(new RoamBehaviour(100, 100, 300, 300)); 
 
 s.getVehicles().add(myCar);
 
 
 
  }
 }
 
